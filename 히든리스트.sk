command /업적변수x:
	permission: 어드민
	trigger:
		if player is op:
			set {왈도특정.%all players%} to 0
			send "모든플레이어의 특정반복대화횟수를 초기화 시켰습니다."
			set {도박히든.%all players%} to 0
			send "모든플레이어의 도박횟수를 초기화 시켰습니다."
			set {킬수.%all players%} to 0
			set {연속 킬수.%all players%} to 0
			send "모든플레이어의 연속 킬수을 초기화 시켰습니다."
			set {현상금.%all players%} to 0
			send "모든플레이어의 현상금을 초기화 시켰습니다."
			set {최대강화횟수.%all players%} to 0
			send "모든플레이어의 총 최대강화횟수을 초기화 시켰습니다."
		else:
			send  "&c당신은 권한이 없습니다."

#업적책 우클릭 이벤트
on rightclick with book:
	#왈도 업적
	if name of player's tool contains "&d&l[&b&l히든리스트1&d&l]": #만약 플레이어가 들고있는 아이템이 히든리스트 1이라면
		broadcast "&e%player%&a님이 &a히든리스트1 &d[&9왈도를 괴롭혀라!&d]&a을 달성하였습니다." #해당 업적 달성 메세지 출력
		log "%now%.%player%히든리스트1사용"
		play sound "ui_toast_challenge_complete" with volume 0.4 and pitch 1.5 at player
		send "히든리스트는 /업적을 통해 확인이 가능합니다." to player
		set {업적1.%player%} to true #/업적의 기록 활성화
		remove 1 book from player's tool
	#도박 업적
	if name of player's tool contains "&d&l[&b&l히든리스트2&d&l]": #만약 플레이어가 들고있는 아이템이 히든리스트 2이라면
		broadcast "&e%player%&a님이 &d&a히든리스트2 &d[&9난 도박왕이야!&d]&a을 달성하였습니다." #해당 업적 달성 메세지 출력
		log "%now%.%player%히든리스트2사용"
		play sound "ui_toast_challenge_complete" with volume 0.4 and pitch 1.5 at player
		send "히든리스트는 /업적을 통해 확인이 가능합니다." to player
		set {업적2.%player%} to true #/업적의 기록 활성화
		remove 1 book from player's tool
	#마켓 업적
	if name of player's tool contains "&d&l[&b&l히든리스트3&d&l]": #만약 플레이어가 들고있는 아이템이 히든리스트 3이라면
		broadcast "&e%player%&a님이 &d&a히든리스트3 &d[&9어멋! 이런곳이?&d]&a을 달성하였습니다." #해당 업적 달성 메세지 출력
		log "%now%.%player%히든리스트3사용"
		play sound "ui_toast_challenge_complete" with volume 0.4 and pitch 1.5 at player
		send "히든리스트는 /업적을 통해 확인이 가능합니다." to player
		set {업적3.%player%} to true #/업적의 기록 활성화
		remove 1 book from player's tool
	#살인마 업적
	if name of player's tool contains "&d&l[&b&l히든리스트4&d&l]": #만약 플레이어가 들고있는 아이템이 히든리스트 4이라면
