#히든리스트 책을 명령어로 얻게하는 명령어
command /히든 [<player>] [<integer>]:
	permission: 어드민
	trigger:
		if player is op:
			if arg-1 is not set:
				send "사용법 : &7/&6히든 &b..." to player
				send "   &b플레이어 이름&7 - &r히든리스트를 지급받을 플레이어 이름을 적습니다." to player
			if arg-1 is "%player%":
				if arg-2 is not set:
					send "사용법 : &7/&6히든 &b플레이어 이름 &4..." to player
					send "  &4 1&7 - &r히든리스트 1 왈도 업적을 지급받습니다." to player
					send "  &4 2&7 - &r히든리스트 2 도박 업적을 지급받습니다." to player
					send "  &4 3&7 - &r히든리스트 3 마켓 업적을 지급받습니다." to player
					send "  &4 4&7 - &r히든리스트 4 살인마 업적을 지급받습니다." to player
					send "  &4 5&7 - &r히든리스트 5 업데이트 예정." to player
					send "  &4 6&7 - &r히든리스트 6 최대 강화 업적을 지급받습니다." to player
			# 왈도 업적
			if arg-2 is 1:
				send "&d히든%arg-2% 활성화"
				set {업적%arg-2%.%arg-1%} to false # 플레이어의 업적변수를 안받음으로 셋팅
				set {왈도신호.%arg-1%} to false # 플레이어의 히든리스트 책을 안받음으로 셋팅 루프문
				set {왈도업적.%arg-1%} to false # 플레이어의 히든리스트 책을 안받음으로 셋팅
			# 도박 업적
			if arg-2 is 2:
				send "&d히든%arg-2% 활성화"
				set {업적%arg-2%.%arg-1%} to false # 플레이어의 업적변수를 안받음으로 셋팅
				set {도박업적.%arg-1%} to false # 플레이어의 히든리스트 책을 안받음으로 셋팅 루프문
				set {도박히든.%arg-1%} to 201
			set {업적%arg-2%.%arg-1%} to false # /업적의 기록 활성화
			# 마켓 업적
			if arg-2 is 3:
				send "&d히든%arg-2% 활성화"
				set {업적%arg-2%.%arg-1%} to false # 플레이어의 업적변수를 안받음으로 셋팅
				set {마켓신호.%arg-1%} to false # 플레이어의 히든리스트 책을 안받음으로 셋팅 루프문
				set {마켓업적.%arg-1%} to false # 플레이어의 히든리스트 책을 안받음으로 셋팅
			# 살인마 업적
			if arg-2 is 4:
				send "&d히든%arg-2% 활성화"
				set {업적%arg-2%.%arg-1%} to false # 플레이어의 업적변수를 안받음으로 셋팅
				set {킬수업적.%arg-1%} to false #플레이어의 히든리스트 책을 안받음으로 셋팅 루프문
				set {연속 킬수.%arg-1%} to 10
			set {업적%arg-2%.%arg-1%} to false # /업적의 기록 활성화
			if arg-2 is 5:
				send "&d업데이트 예정"
			if arg-2 is 6:
				send "&d히든%arg-2% 활성화"
				set {업적%arg-2%.%arg-1%} to false # 플레이어의 업적변수를 안받음으로 셋팅
				set {강화업적1신호.%arg-1%} to false #플레이어의 히든리스트 책을 못얻게 막기
				set {최대강화횟수.%arg-1%} to 1
		else:
			send  "&c당신은 권한이 없습니다."

# 히든리스트 변수를 얻었다로 바꾸는 명령어
command /히든권o [<player>] [<integer>]:
	permission: 어드민
	trigger:
		if player is op:
			if arg-1 is not set:
				send "사용법 : &7/&6히든권o &b..." to player
				send "   &b플레이어 이름&7 - &r히든리스트를 업적을 해금시킬 플레이어 이름을 적습니다." to player
			if arg-1 is "%player%":
				if arg-2 is not set:
					send "사용법 : &7/&6히든권o &b플레이어 이름 &4..." to player
					send "  &4 1&7 - &r히든리스트1 왈도 업적을 해금 시킵니다." to player
					send "  &4 2&7 - &r히든리스트2 도박 업적을 해금 시킵니다." to player
					send "  &4 3&7 - &r히든리스트3 마켓 업적을 해금 시킵니다." to player
					send "  &4 4&7 - &r히든리스트4 살인마 업적을 해금 시킵니다." to player
					send "  &4 5&7 - &r히든리스트5 업데이트예정." to player
					send "  &4 6&7 - &r히든리스트6 최대강화 업적을 해금 시킵니다." to player
			# 왈도 업적
			if arg-2 is 1: # arg-1 = 플레이어 이름 / arg-2 = 업적번호
				set {업적%arg-2%.%arg-1%} to true # 플레이어의 업적변수를 받음으로 셋팅
				set {왈도업적.%arg-1%} to true # 플레이어의 히든리스트 책을 못얻게 막기
				set {왈도신호.%arg-1%} to true # 플레이어의 히든리스트 책을 못얻게 막기 루프문
				send "&d[&b히든리스트%arg-2%&d]업적을 얻으셨습니다." to player
			set {업적%arg-2%.%arg-1%} to true # /업적의 기록 활성화
			# 도박 업적
			if arg-2 is 2: # arg-1 = 플레이어 이름 / arg-2 = 업적번호
				set {업적%arg-2%.%arg-1%} to true # 플레이어의 업적변수를 받음으로 셋팅
				set {도박업적.%arg-1%} to true # 플레이어의 히든리스트 책을 못얻게 막기
				send "&d[&b히든리스트%arg-2%&d]업적을 얻으셨습니다." to player
			set {업적%arg-2%.%arg-1%} to true # /업적의 기록 활성화
			# 마켓 업적
			if arg-2 is 3: # arg-1 = 플레이어 이름 / arg-2 = 업적번호
				set {업적%arg-2%.%arg-1%} to true # 플레이어의 업적변수를 받음으로 셋팅
				set {마켓업적.%arg-1%} to true # 플레이어의 히든리스트 책을 못얻게 막기
				set {마켓신호.%arg-1%} to true # 플레이어의 히든리스트 책을 못얻게 막기 루프문
				send "&d[&b히든리스트%arg-2%&d]업적을 얻으셨습니다." to player
			set {업적%arg-2%.%arg-1%} to true # /업적의 기록 활성화
			# 살인마 업적
			if arg-2 is 4: # arg-1 = 플레이어 이름 / arg-2 = 업적번호
				set {업적%arg-2%.%arg-1%} to true # 플레이어의 업적변수를 받음으로 셋팅
				set {킬수업적.%arg-1%} to true # 플레이어의 히든리스트 책을 못얻게 막기
				send "&d[&b히든리스트%arg-2%&d]업적을 얻으셨습니다." to player
			set {업적%arg-2%.%arg-1%} to true # /업적의 기록 활성화
			if arg-2 is 5:
				send "업데이트 예정" to player
			if arg-2 is 6:
				set {업적%arg-2%.%arg-1%} to true # 플레이어의 업적변수를 받음으로 셋팅
				set {강화업적1신호.%arg-1%} to true # 플레이어의 업적변수를 받음으로 셋팅
				send "&d[&b히든리스트%arg-2%&d]업적을 얻으셨습니다." to player
		else:
			send  "&c당신은 권한이 없습니다."

