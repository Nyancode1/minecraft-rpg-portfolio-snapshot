# 나무 무기 등급업
every 1 second:
	loop all players:
		if {나무등급업.%loop-player%} is set:
			set {_장비스탯::*} to uncolored lore of slot 10 of current inventory of  loop-player
			replace all " " and "+" and "공격력" with "" in {_장비스탯::4} # 공격력 확인
			replace all " " and "기본" and "추가" and "공격력" with "" in {_장비스탯::6} #기본 추가 공격력 확인
			set {_강화공격력} to {_장비스탯::4} parsed as number
			set {_기본공격력} to {_장비스탯::6} parsed as number
			replace all "&a" with "&9" in {이름.%loop-player%}
			send "%{이름.%loop-player%}%" to loop-player
			message "&e&l단야사&f&l : &a&l이보게나! 등급업에 성공했다네. " to loop-player
			set {_w} to nbt of slot 10 of current inventory of loop-player
			delete slot 10 of current inventory of loop-player
			if {이름.%loop-player%} is "&d&l[&9&l건틀릿&d&l]":
				set slot 16 of current inventory of loop-player to stone sword with nbt compound from "%{_w}%" named "%{이름.%loop-player%}%" with lore "&e★&f★" and "&b0단계" and "&e==============" and "&c + 공격력 %{_강화공격력}%" and "&e==============" and "&2기본 추가 공격력 %{_기본공격력} + 0.2%"
				delete {이름.%loop-player%}
				delete {나무등급업.%loop-player%}
			else if {이름.%loop-player%} is "&d&l[&9&l단검&d&l]":
				set slot 16 of current inventory of loop-player to stone sword with nbt compound from "%{_w}%" named "%{이름.%loop-player%}%" with lore "&e★&f★" and "&b0단계" and "&e==============" and "&c + 공격력 %{_강화공격력}%" and "&e==============" and "&2기본 추가 공격력 %{_기본공격력} + 0.5%"
				delete {이름.%loop-player%}
				delete {나무등급업.%loop-player%}
			else if {이름.%loop-player%} is "&d&l[&9&l낫&d&l]":
				set slot 16 of current inventory of loop-player to stone sword with nbt compound from "%{_w}%" named "%{이름.%loop-player%}%" with lore "&e★&f★" and "&b0단계" and "&e==============" and "&c + 공격력 %{_강화공격력}%" and "&e==============" and "&2기본 추가 공격력 %{_기본공격력} + 2.5%"
				delete {이름.%loop-player%}
				delete {나무등급업.%loop-player%}
			else if {이름.%loop-player%} is "&d&l[&9&l대검&d&l]":
				set slot 16 of current inventory of loop-player to stone sword with nbt compound from "%{_w}%" named "%{이름.%loop-player%}%" with lore "&e★&f★" and "&b0단계" and "&e==============" and "&c + 공격력 %{_강화공격력}%" and "&e==============" and "&2기본 추가 공격력 %{_기본공격력} + 2%"
				delete {이름.%loop-player%}
				delete {나무등급업.%loop-player%}
			else if {이름.%loop-player%} is "&d&l[&9&l장검&d&l]":
				set slot 16 of current inventory of loop-player to stone sword with nbt compound from "%{_w}%" named "%{이름.%loop-player%}%" with lore "&e★&f★" and "&b0단계" and "&e==============" and "&c + 공격력 %{_강화공격력}%" and "&e==============" and "&2기본 추가 공격력 %{_기본공격력} + 1%"
				delete {이름.%loop-player%}
				delete {나무등급업.%loop-player%}
			else if {이름.%loop-player%} is "&d&l[&9&l지팡이&d&l]":
				set slot 16 of current inventory of loop-player to stone sword with nbt compound from "%{_w}%" named "%{이름.%loop-player%}%" with lore "&e★&f★" and "&b0단계" and "&e==============" and "&c + 공격력 %{_강화공격력}%" and "&e==============" and "&2기본 추가 공격력 %{_기본공격력} + 0.7%"
				delete {이름.%loop-player%}
				delete {나무등급업.%loop-player%}
			else:
				delete {이름.%loop-player%}
				delete {나무등급업.%loop-player%}
				wait 0.8 second
				message "&e&l단야사&f&l : &a&l어라..? 자네 일반 무기였나 " to loop-player
				wait 0.8 second
				message "&e&l단야사&f&l : &a&l미안하지만 특수한 기운이 담긴 무기만을 등급업을 시킬 수 있네" to loop-player
				wait 0.8 second
				message "&e&l단야사&f&l : &a&l하지만 이 무기는 무엇인가 변했군 일단 다시 돌려주겠네." to loop-player
				#set 
				set slot 16 of current inventory of loop-player to wooden sword with nbt compound from "%{_w}%" named "%{이름.%loop-player%}%" with lore "&c&o등급업불가?" and "&b&k7&b단계" and "&e==============" and "&c + 공&k격&c력 %{_강화공격력}%" and "&e==============" and "&2기본 &k추가 &2공격&k력&2 &k%{_기본공격력}%"

