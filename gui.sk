command /출변:
	trigger:
		set {출입 변수.%player%} to true
		set {출입 변수1.%player%} to true
		set {출입 변수2.%player%} to true
		set {출입 변수3.%player%} to true

on sneak toggle:
	player is not sneaking:
		set {shift.%player%} to false
	else:
		set {shift.%player%} to true

on swap hand item:
	if {shift.%player%} is false:
		cancel event
		open chest with 3 row named "&9&l내정보" to player
		set {_slot} to 0
		loop 9 times:
			set slot {_slot} of current inventory of player to blue stained glass pane
			add 1 to {_slot}
		set {_slot} to 9
		loop 17 times:
			set slot {_slot} of current inventory of player to black stained glass pane
			add 1 to {_slot}
		set {_slot} to 18
		loop 27 times:
			set slot {_slot} of current inventory of player to blue stained glass pane
			add 1 to {_slot}
		set slot 10 of current inventory of player to iron chestplate named "&a&l장비 창" with lore "&9&l누르면 장비 창을 엽니다."
		set slot 12 of current inventory of player to grass block named "&a&l텔레포트" with lore "&9&l누르면 텔레포트 창을 엽니다."
		set slot 14 of current inventory of player to writable book named "&a&l업적" with lore "&9&l누르면 업적 보관함을 엽니다."
		set slot 16 of current inventory of player to name tag named "&a&l칭호 보관소" with lore "&9&l누르면 칭호 보관함을 엽니다."
